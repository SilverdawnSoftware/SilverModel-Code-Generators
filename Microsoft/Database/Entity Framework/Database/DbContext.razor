@using SilvaDawn.SilvaModel.UML.Extensions;
@using SilvaDawn.SilvaModel.UML.Interfaces.Classes;
@using SilvaDawn.SilvaModel.UML.Classes;
// ALLOWOVERWRITE
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Data.Entity;
using @(Model.Diagram.Name).Database;

namespace @(Model.Diagram.Name).Database
{
    public class @Model.Diagram["Entity Framework","DBContextName"] : DbContext
    {
    
    @foreach (var att in @Model.Diagram.Classes())
    {
        <text>public DbSet<@att.Name></text> @att.Name<text>s { get; set; } </text>
     }
        
     }
 }