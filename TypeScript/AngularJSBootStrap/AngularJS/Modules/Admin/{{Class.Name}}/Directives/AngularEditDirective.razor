@using SilvaDawn.SilvaModel.UML.Extensions;
@using SilvaDawn.SilvaModel.UML.Interfaces.Classes;
@using SilvaDawn.SilvaModel.UML.Classes;
// ALLOWOVERWRITE


import { IHttpService, IPromise, IScope, IQService, ILogService,IDirective } from 'angular';

import * as angular from "angular";

module Admin {

    

    export function @(Model.Classifier.Name)EditDirective(): IDirective {

        return { 
            templateUrl: '/AngularJS/Modules/Admin/@(Model.Classifier.Name)/Directives/@(Model.Classifier.Name)EditDirective.html',
            replace: true,
            controller: '@(Model.Classifier.Name.LowerFirst())EditDirectiveCtrl',
            controllerAs: 'ctrl',
            scope: true
        }
    }

    angular.module("@(Model.Diagram.Name).Admin").directive('@(Model.Classifier.Name.LowerFirst())Edit', @(Model.Classifier.Name)EditDirective);
}
 