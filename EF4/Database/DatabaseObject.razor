@using SilvaDawn.SilvaModel.UML.Extensions;
@using SilvaDawn.SilvaModel.UML.Interfaces.Classes;
@using SilvaDawn.SilvaModel.UML.Classes;
// ALLOWOVERWRITE

using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;


namespace @(Model.Model.QualifiedName).Database
{
    public class @Model.Classifier.Name
    {
    	@foreach (var att in Model.Classifier.Properties())
    	{ 
    	<text>public @Model.RazorSupport.LookupDataType("C#",att.DataType)</text> @att.Name <text> { get; set; } </text>
    	}
    	
    	@foreach (var att in Model.Classifier.GetAssociations())
    	{ 
    	<text>public virtual List<@att.GetTargetMemberEnd().Type.Name></text> @att.GetTargetMemberEnd().Type.Name<text>s { get; set; } </text>
    	}
    	
    	@foreach (var att in Model.Classifier.GetTargetAssociation())
    	{ 
    	<text>public virtual @att.GetSourceMemberEnd().Type.Name</text> @att.GetSourceMemberEnd().Type.Name<text> { get; set; } </text>
    	}
    	
    }
    
    
}